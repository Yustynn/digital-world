<MenuScreen>:
    BoxLayout:
        Button:
            text: 'Settings'
            on_press: root.manager.current = 'settings'

        Button:
            text: 'Quit'
            on_press: app.stop()

<SettingsScreen>:
    BoxLayout:
        Button:
            text: 'Settings Screen'
        Button:
            text: 'Back to Menu'
            on_press: root.manager.current = 'menu'
